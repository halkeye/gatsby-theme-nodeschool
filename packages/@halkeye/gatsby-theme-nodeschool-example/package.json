{
  "name": "@halkeye/gatsby-theme-nodeschool-example",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "1.0.0",
  "author": "Gavin Mogan <npm@gavinmogan.com>",
  "dependencies": {
    "gatsby": "5.14.1",
    "gatsby-plugin-manifest": "5.14.0",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-source-filesystem": "5.14.0",
    "gatsby-theme-nodeschool": "*",
    "gh-pages": "6.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint --ext .js --ext .jsx .",
    "build": "TZ=UTC gatsby build --prefix-paths",
    "clean": "gatsby clean && rm -rf public",
    "develop": "gatsby develop -p 3000 -H 0.0.0.0",
    "deploy:github": "gh-pages -d public",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "eslint": "8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-promise": "7.1.0",
    "jest": "29.7.0"
  },
  "workspaces": [
    "../gatsby-theme-nodeschool"
  ]
}
