{
  "name": "@halkeye/gatsby-theme-nodeschool-example",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "1.0.0",
  "author": "Gavin Mogan <npm@gavinmogan.com>",
  "dependencies": {
    "gatsby": "4.8.1",
    "gatsby-plugin-manifest": "4.8.1",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-source-filesystem": "4.8.1",
    "gatsby-theme-nodeschool": "workspace:*",
    "gh-pages": "3.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint --ext .js --ext .jsx .",
    "build": "TZ=UTC gatsby build --prefix-paths",
    "clean": "gatsby clean && rm -rf public",
    "develop": "gatsby develop -p 3000 -H 0.0.0.0",
    "deploy:github": "gh-pages -d public",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/eslint-plugin": "7.16.5",
    "eslint": "8.10.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-promise": "6.0.0",
    "gatsby": "4.8.1",
    "jest": "27.5.1"
  },
  "workspaces": [
    "../gatsby-theme-localization",
    "../gatsby-theme-nodeschool"
  ]
}
