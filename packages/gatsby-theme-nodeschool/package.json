    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^5.9.0",
    "gatsby-plugin-slug": "^2.0.0",
    "gatsby-remark-copy-linked-files": "^6.12.0",
    "gatsby-remark-images": "^7.9.0",
    "gatsby-remark-smartypants": "^6.12.0",
    "gatsby-source-filesystem": "^5.9.0",
    "gatsby-source-meetup": "^1.1.0",
    "gatsby-transformer-sharp": "^5.9.0",
    "gatsby-transformer-yaml": "^5.9.0",
    "i18next": "^23.0.0",
    "loader-utils": "3.3.1",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.0.0",
    "prop-types": "^15.8.1",
    "react-helmet": "^6.1.0",
    "react-hexagon": "^1.1.3",
    "react-i18next": "^12.2.0",
    "sass": "^1.62.0"
