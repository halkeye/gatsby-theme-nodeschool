{
  "name": "gatsby-theme-nodeschool",
  "description": "Gatsby Theme for NodeSchool",
  "version": "2.0.0",
  "author": "Gavin Mogan <npm@gavinmogan.com>",
  "homepage": "https://halkeye.github.io/gatsby-theme-nodeschool/",
  "main": "index.js",
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "release": {
    "branches": [
      "master",
      "next"
    ]
  },
  "files": [
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "gatsby-ssr.js",
    "index.js",
    "README.md",
    "src"
  ],
  "scripts": {
    "extract": "NODE_ENV=test npx babel --config-file ./babel-extract.config.js -o tmp/chunk.js 'src/**/*.{js,jsx,ts,tsx}' && rm -rf tmp",
    "precommit": "lint-staged",
    "lint": "eslint --ext .js --ext .jsx .",
    "test": "jest"
  },
  "engine": {
    "npm": ">= 6",
    "node": ">= 18"
  },
  "dependencies": {
    "@halkeye/gatsby-plugin-google-fonts-v2": "^1.0.2",
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "dateformat": "^5.0.3",
    "gatsby-plugin-catch-links": "^5.9.0",
    "gatsby-plugin-mdx": "^5.9.0",
    "gatsby-plugin-offline": "^6.12.0",
    "gatsby-plugin-postcss": "^6.12.0",
    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-react-i18next": "^3.0.1",
